{
  "name": "project-zero",
  "private": true,
  "version": "0.0.0",
  "description": "GitVision Pro - análise avançada de atividade no GitHub em uma SPA React + TypeScript.",
  "author": "Enoque Sousa (https://enoquesousa.vercel.app)",
  "homepage": "https://project-zero-seven.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/ESousa97/project-zero.git"
  },
  "bugs": {
    "url": "https://github.com/ESousa97/project-zero/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "minimatch": "^10.2.2"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview",
    "validate": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^2.12.7"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@tailwindcss/postcss": "^4.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "autoprefixer": "^10.4.24",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "tailwindcss": "^4.2.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^7.0.4",
    "vitest": "^4.0.18"
  }
}
